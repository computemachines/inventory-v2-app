    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "query-string": "^6.13.0",
    "react-hot-loader": "^4.12.21",
    "redux": "^4.0.5",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "svg-inline-loader": "^0.8.2",
    "svg-inline-react": "^3.2.0",
    "terser-webpack-plugin": "^3.0.3",
    "webpack": "^4.43.0",
