{
  "name": "inventory-v2",
  "version": "2.0.0",
  "scripts": {
    "format": "prettier \"src/**/*.{js,jsx,html}\" webpack*.js --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "build:client:dev": "webpack --config webpack.client.development.config.js",
    "build:client": "webpack --config webpack.client.production.config.js",
    "build:server": "webpack --config webpack.server.config.js",
    "start:dev": "webpack-dev-server --config webpack.client.development.config.js --hot",
    "start:ssr": "node server.js",
    "start:ssr:dev": "node server.js -p 8080",
    "start:ssr:only": "node server.js -p 8080 --noclient",
    "start:client:only": "http-server \"dist/assets\"",
    "ssr:dev": "npm run build:client && npm run build:server && npm run start:ssr:dev",
    "start": "npm run start:ssr"
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^3.5.3",
    "docopt": "^0.6.2",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "less": "^3.11.1",
    "less-loader": "^6.0.0",
    "mercury": "^14.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "request": "^2.88.2",
    "require-from-string": "^2.0.2",
    "style-loader": "^1.2.1",
    "svg-inline-loader": "^0.8.2",
    "svg-inline-react": "^3.2.0 ",
    "terser-webpack-plugin": "^3.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "hookrouter": "^1.2.3",
    "http-server": "^0.12.3",
    "prettier": "^2.0.5"
  }
}
