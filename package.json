    "ssr:only": "node server.js -p 8080 --noclient",
    "client:only": "http-server \"dist/assets\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "cross-env": "^7.0.2",
    "docopt": "^0.6.2",
