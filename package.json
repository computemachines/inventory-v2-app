{
  "name": "inventory-v2",
  "version": "2.0.0",
  "scripts": {
    "format": "prettier \"src/**/*.{js,jsx,html}\" webpack*.js --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "build:dev": "webpack",
    "build": "cross-env NODE_ENV='production' webpack",
    "dev": "webpack-dev-server --hot --history-api-fallback --open",
    "ssr": "node server.js",
