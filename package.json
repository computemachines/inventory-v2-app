{
  "name": "inventory-v2",
  "version": "2.0.0",
  "scripts": {
    "format": "prettier \"src/**/*.{js,jsx,html}\" webpack*.js --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "build:client:dev": "webpack --config webpack.client.development.config.js",
    "build:client": "webpack --config webpack.client.production.config.js",
    "build:server": "webpack --config webpack.server.config.js",
    "start:dev": "webpack-dev-server --config webpack.client.development.config.js",
    "start:ssr": "node server.js",
    "start:ssr:dev": "node server.js -p 8080",
    "ssr:dev": "npm run build:client && npm run build:server && npm run start:ssr:dev",
    "start": "npm run start:ssr"
  },
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^3.1.0",
    "docopt": "^0.6.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.1.0",
    "express": "^4.17.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mercury": "^14.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^5.1.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.4",
    "request": "^2.88.0",
    "require-from-string": "^2.0.2",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "svg-inline-react": "^3.2.0",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prettier": "^1.19.1"
  }
}
