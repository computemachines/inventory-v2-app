{
  "extends": [
    "airbnb",
    "prettier",
  ],
  "parser": "babel-eslint",
  "plugins": [ "prettier", "react" ],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "react/destructuring-assignment": 0
  }
}
