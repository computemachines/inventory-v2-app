{
  "ecmaVersion": "8",
  "libs": [
    "react",
    "browser"
  ],
  "loadEagerly": [
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "doc_comment": true,
    "webpack": {
      "configPath": "./webpack.client.development.config.js"
    },
    "es_modules": {},
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    }
  }
}